{
  "$schema": "https://fairspec.org/profiles/0.1.0/table.json",
  "title": "Car",
  "description": "A table schema describing a car",
  "primaryKey": ["url"],
  "foreignKeys": [
    {
      "columns": ["showroomId"],
      "reference": {
        "resource": "showroom",
        "columns": ["id"]
      }
    }
  ],
  "required": [
    "title",
    "url",
    "price",
    "currency",
    "mileage",
    "brand",
    "model",
    "version",
    "fuel",
    "gearbox",
    "category",
    "color",
    "door"
  ],
  "properties": {
    "showroomId": {
      "type": "string",
      "description": "Unique identifier for the showroom where the car is located. If not provided the car is considered located at the dealers's main address",
      "examples": ["showroom-downtown-123"]
    },
    "title": {
      "type": "string",
      "description": "The title or name of the car listing",
      "examples": ["2023 Tesla Model 3 Long Range"]
    },
    "url": {
      "type": "string",
      "format": "url",
      "description": "URL to the car listing",
      "examples": ["https://example.com/cars/tesla-model-3-2025"]
    },
    "price": {
      "type": "number",
      "description": "The price of the car in the currency specified",
      "examples": [45990],
      "minimum": 0
    },
    "currency": {
      "type": "string",
      "description": "Currency of the price",
      "examples": ["EUR"]
    },
    "year": {
      "type": "integer",
      "description": "Year of first registration (1900-2100)",
      "examples": [2023],
      "minimum": 1900
    },
    "mileage": {
      "type": "number",
      "description": "Odometer reading in kilometers",
      "examples": [15000],
      "minimum": 0
    },
    "brand": {
      "type": "string",
      "description": "Car brand/manufacturer",
      "examples": ["Tesla"]
    },
    "model": {
      "type": "string",
      "description": "Car model name",
      "examples": ["Model 3"]
    },
    "version": {
      "type": "string",
      "description": "Specific version or trim level",
      "examples": ["Long Range AWD"]
    },
    "fuel": {
      "type": "string",
      "description": "Fuel type",
      "examples": ["electric"],
      "enum": ["diesel", "petrol", "hybrid", "electric"]
    },
    "gearbox": {
      "type": "string",
      "description": "Transmission type",
      "examples": ["auto"],
      "enum": ["manual", "auto"]
    },
    "category": {
      "type": "string",
      "description": "Vehicle category/body type",
      "examples": ["suv"],
      "enum": ["small", "estate", "saloon", "suv", "van", "sport", "cabrio"]
    },
    "color": {
      "type": "string",
      "description": "Exterior color",
      "examples": ["white"],
      "enum": [
        "grey",
        "black",
        "white",
        "blue",
        "red",
        "green",
        "yellow",
        "orange"
      ]
    },
    "door": {
      "type": "string",
      "description": "Number of doors identifier",
      "examples": ["fourfive"],
      "enum": ["twothree", "fourfive", "sixseven"]
    },
    "power": {
      "type": "integer",
      "description": "Engine power in horsepower",
      "examples": [283]
    },
    "cubics": {
      "type": "integer",
      "description": "Engine displacement in cubic centimeters",
      "examples": [1998]
    },
    "seats": {
      "type": "integer",
      "description": "Number of seats",
      "examples": [5]
    },
    "owners": {
      "type": "integer",
      "description": "Number of previous owners",
      "examples": [1]
    },
    "month": {
      "type": "integer",
      "description": "Month of first registration (1-12)",
      "examples": [6],
      "minimum": 1,
      "maximum": 12
    },
    "warranty": {
      "type": "integer",
      "description": "Warranty duration in months",
      "examples": [24]
    },
    "range": {
      "type": "integer",
      "description": "Electric vehicle range in kilometers",
      "examples": [580]
    },
    "battery": {
      "type": "integer",
      "description": "Battery capacity in kWh for electric vehicles",
      "examples": [75]
    },
    "plate": {
      "type": "string",
      "description": "License plate number",
      "examples": ["AB-12-CD"]
    },
    "vin": {
      "type": "string",
      "description": "Vehicle Identification Number",
      "examples": ["5YJ3E1EA4KF123456"]
    }
  }
}
